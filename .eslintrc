{
	"extends": [
		"@geostarters",
		"plugin:react/recommended"
	],
	"settings": {
		"react": {
			"version": "detect"
		},
	},
	"parserOptions": {
		"sourceType": "module"
	},
	"overrides": [
	  {
		"files": [
		  "**/*.test.js"
		],
		"env": {
		  "jest": true
		},
		"plugins": ["jest"],
		"rules": {
		  "jest/no-disabled-tests": "warn",
		  "jest/no-focused-tests": "error",
		  "jest/no-identical-title": "error",
		  "jest/prefer-to-have-length": "warn",
		  "jest/valid-expect": "error"
		}
	  }
	]
  }
  
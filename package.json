{
  "name": "@geostarters/react-components",
  "version": "0.0.5",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@geostarters/eslint-config": "^0.0.1",
    "@storybook/addon-info": "^4.1.2",
    "@storybook/react": "^4.1.1",
    "@storybook/storybook-deployer": "^2.8.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.3.1",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "react-scripts": "1.1.4",
    "react-test-renderer": "^16.3.2",
    "rimraf": "^2.6.3"
  },
  "peerDependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "scripts": {
    "lint": "eslint --fix --cache --ignore-path .gitignore src",
    "build:es-dev": "set NODE_ENV=production&babel ./src --out-dir dist/es/ --copy-files",
    "build:es": "NODE_ENV=production babel ./src --out-dir dist/es/ --copy-files",
    "build:commonjs-dev": "set NODE_ENV=production&babel --plugins @babel/plugin-transform-modules-commonjs dist/es/ --out-dir dist/commonjs/",
    "build:commonjs": "NODE_ENV=production babel --plugins @babel/plugin-transform-modules-commonjs dist/es/ --out-dir dist/commonjs/",
    "build:dev": "run-s clean:dist lint build:es-dev clean:files build:commonjs-dev",
    "build": "run-s clean:dist lint build:es clean:files build:commonjs",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy-storybook": "storybook-to-ghpages -o docs",
    "build-storybook": "build-storybook -s docs",
    "test-original": "node scripts/test.js --env=jsdom",
    "test": "react-scripts test --env=jsdom",
    "precommit": "npm run lint",
    "clean:files": "rimraf dist/es/setupTests.js dist/es/stories dist/es/components/*.test.js dist/es/components/*/*.test.* dist/es/components/*/__snapshots__ ",
    "clean": "run-s clean:dist",
    "clean:dist": "rimraf dist/*"
  },
  "dependencies": {
    "@geostarters/common": "^0.1.29",
    "mapbox-gl": "^0.52.0-beta.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.84.0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
